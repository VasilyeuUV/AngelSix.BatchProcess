<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AngelSix.BatchProcess.Views.Pages.MacrosPageView"
             
             Foreground="White"
             Background="Black"
             xmlns:vm="clr-namespace:AngelSix.BatchProcess.ViewModels.Pages"
             x:DataType="vm:MacrosPageViewModel"
             >
  <Design.DataContext>
    <vm:MacrosPageViewModel/>
  </Design.DataContext>

  <Label Content="{Binding PageName}"/>

</UserControl>
